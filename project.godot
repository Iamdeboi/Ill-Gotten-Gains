; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Ill-Gotten Gains"
config/description="Game development project dedicated learning to ultlize algorithims in game design, and improving understanding of UI/UX elements within Godot and as a game design principle."
run/main_scene="res://Scenes/Menus/main_menu.tscn"
config/features=PackedStringArray("4.4", "Forward Plus")
config/icon="res://Assets/art/tile_0118.png"

[autoload]

EventBus="*res://Globals/event_bus.gd"

[debug]

settings/stdout/verbose_stdout=true

[display]

window/size/viewport_width=640
window/size/viewport_height=360
window/size/mode=3
window/size/window_width_override=1920
window/size/window_height_override=1080
window/stretch/mode="viewport"
window/stretch/aspect="expand"
window/stretch/scale_mode="integer"
window/snap/snap_2d_transforms_to_pixel=false

[file_customization]

folder_colors={
"res://Scenes/Battle/": "red",
"res://Scenes/GUI/": "green",
"res://Scenes/Menus/": "blue",
"res://Scenes/Utility/": "purple"
}

[global_group]

